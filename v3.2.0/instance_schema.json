{
 "$schema": "http://www.archivesspace.org/archivesspace.json",
 "properties": {
  "create_time": {
   "readonly": true,
   "type": "date-time"
  },
  "created_by": {
   "readonly": true,
   "type": "string"
  },
  "digital_object": {
   "properties": {
    "_resolved": {
     "readonly": "true",
     "type": "object"
    },
    "ref": {
     "ifmissing": "error",
     "type": "JSONModel(:digital_object) uri"
    }
   },
   "subtype": "ref",
   "type": "object"
  },
  "instance_type": {
   "dynamic_enum": "instance_instance_type",
   "ifmissing": "error",
   "minLength": 1,
   "type": "string"
  },
  "is_representative": {
   "default": false,
   "type": "boolean"
  },
  "jsonmodel_type": {
   "ifmissing": "error",
   "type": "string"
  },
  "last_modified_by": {
   "readonly": true,
   "type": "string"
  },
  "lock_version": {
   "required": false,
   "type": [
    "integer",
    "string"
   ]
  },
  "repository": {
   "properties": {
    "_resolved": {
     "readonly": "true",
     "type": "object"
    },
    "ref": {
     "ifmissing": "error",
     "readonly": "true",
     "type": "JSONModel(:repository) uri"
    }
   },
   "readonly": "true",
   "subtype": "ref",
   "type": "object"
  },
  "sub_container": {
   "type": "JSONModel(:sub_container) object"
  },
  "system_mtime": {
   "readonly": true,
   "type": "date-time"
  },
  "user_mtime": {
   "readonly": true,
   "type": "date-time"
  }
 },
 "type": "object",
 "validations": [
  [
   "error",
   "check_instance"
  ]
 ],
 "version": 1
}