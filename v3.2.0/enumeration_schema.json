{
 "$schema": "http://www.archivesspace.org/archivesspace.json",
 "properties": {
  "create_time": {
   "readonly": true,
   "type": "date-time"
  },
  "created_by": {
   "readonly": true,
   "type": "string"
  },
  "default_value": {
   "type": "string"
  },
  "editable": {
   "readonly": true,
   "type": "boolean"
  },
  "enumeration_values": {
   "items": {
    "type": "JSONModel(:enumeration_value) object"
   },
   "type": "array"
  },
  "jsonmodel_type": {
   "ifmissing": "error",
   "type": "string"
  },
  "last_modified_by": {
   "readonly": true,
   "type": "string"
  },
  "lock_version": {
   "required": false,
   "type": [
    "integer",
    "string"
   ]
  },
  "name": {
   "ifmissing": "error",
   "maxLength": 255,
   "type": "string"
  },
  "readonly_values": {
   "items": {
    "type": "string"
   },
   "readonly": true,
   "type": "array"
  },
  "relationships": {
   "items": {
    "type": "string"
   },
   "type": "array"
  },
  "repository": {
   "properties": {
    "_resolved": {
     "readonly": "true",
     "type": "object"
    },
    "ref": {
     "ifmissing": "error",
     "readonly": "true",
     "type": "JSONModel(:repository) uri"
    }
   },
   "readonly": "true",
   "subtype": "ref",
   "type": "object"
  },
  "system_mtime": {
   "readonly": true,
   "type": "date-time"
  },
  "uri": {
   "required": false,
   "type": "string"
  },
  "user_mtime": {
   "readonly": true,
   "type": "date-time"
  },
  "values": {
   "ifmissing": "error",
   "items": {
    "type": "string"
   },
   "type": "array"
  }
 },
 "type": "object",
 "uri": "/config/enumerations",
 "version": 1
}