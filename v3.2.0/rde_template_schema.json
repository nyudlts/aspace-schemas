{
 "$schema": "http://www.archivesspace.org/archivesspace.json",
 "properties": {
  "create_time": {
   "readonly": true,
   "type": "date-time"
  },
  "created_by": {
   "readonly": true,
   "type": "string"
  },
  "defaults": {
   "type": "object"
  },
  "jsonmodel_type": {
   "ifmissing": "error",
   "type": "string"
  },
  "last_modified_by": {
   "readonly": true,
   "type": "string"
  },
  "lock_version": {
   "required": false,
   "type": [
    "integer",
    "string"
   ]
  },
  "name": {
   "ifmissing": "error",
   "type": "string"
  },
  "order": {
   "items": {
    "type": "string"
   },
   "type": "array"
  },
  "record_type": {
   "enum": [
    "archival_object",
    "digital_object_component"
   ],
   "ifmissing": "error",
   "type": "string"
  },
  "repository": {
   "properties": {
    "_resolved": {
     "readonly": "true",
     "type": "object"
    },
    "ref": {
     "ifmissing": "error",
     "readonly": "true",
     "type": "JSONModel(:repository) uri"
    }
   },
   "readonly": "true",
   "subtype": "ref",
   "type": "object"
  },
  "system_mtime": {
   "readonly": true,
   "type": "date-time"
  },
  "uri": {
   "required": false,
   "type": "string"
  },
  "user_mtime": {
   "readonly": true,
   "type": "date-time"
  },
  "visible": {
   "items": {
    "type": "string"
   },
   "type": "array"
  }
 },
 "type": "object",
 "uri": "/repositories/:repo_id/rde_templates",
 "version": 1
}