{
 "$schema": "http://www.archivesspace.org/archivesspace.json",
 "properties": {
  "create_time": {
   "readonly": true,
   "type": "date-time"
  },
  "created_by": {
   "readonly": true,
   "type": "string"
  },
  "date": {
   "ifmissing": "error",
   "maxLength": 255,
   "type": "string"
  },
  "description": {
   "ifmissing": "error",
   "maxLength": 65000,
   "type": "string"
  },
  "jsonmodel_type": {
   "ifmissing": "error",
   "type": "string"
  },
  "last_modified_by": {
   "readonly": true,
   "type": "string"
  },
  "lock_version": {
   "required": false,
   "type": [
    "integer",
    "string"
   ]
  },
  "publish": {
   "type": "boolean"
  },
  "repository": {
   "properties": {
    "_resolved": {
     "readonly": "true",
     "type": "object"
    },
    "ref": {
     "ifmissing": "error",
     "readonly": "true",
     "type": "JSONModel(:repository) uri"
    }
   },
   "readonly": "true",
   "subtype": "ref",
   "type": "object"
  },
  "system_mtime": {
   "readonly": true,
   "type": "date-time"
  },
  "uri": {
   "required": false,
   "type": "string"
  },
  "user_mtime": {
   "readonly": true,
   "type": "date-time"
  }
 },
 "type": "object",
 "uri": "/revision_statement",
 "version": 1
}