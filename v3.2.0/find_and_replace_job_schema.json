{
 "$schema": "http://www.archivesspace.org/archivesspace.json",
 "properties": {
  "base_record_uri": {
   "ifmissing": "error",
   "type": "string"
  },
  "create_time": {
   "readonly": true,
   "type": "date-time"
  },
  "created_by": {
   "readonly": true,
   "type": "string"
  },
  "find": {
   "ifmissing": "error",
   "type": "string"
  },
  "jsonmodel_type": {
   "ifmissing": "error",
   "type": "string"
  },
  "last_modified_by": {
   "readonly": true,
   "type": "string"
  },
  "lock_version": {
   "required": false,
   "type": [
    "integer",
    "string"
   ]
  },
  "property": {
   "ifmissing": "error",
   "type": "string"
  },
  "record_type": {
   "ifmissing": "error",
   "type": "string"
  },
  "replace": {
   "ifmissing": "error",
   "type": "string"
  },
  "repository": {
   "properties": {
    "_resolved": {
     "readonly": "true",
     "type": "object"
    },
    "ref": {
     "ifmissing": "error",
     "readonly": "true",
     "type": "JSONModel(:repository) uri"
    }
   },
   "readonly": "true",
   "subtype": "ref",
   "type": "object"
  },
  "system_mtime": {
   "readonly": true,
   "type": "date-time"
  },
  "user_mtime": {
   "readonly": true,
   "type": "date-time"
  }
 },
 "type": "object",
 "validations": [
  [
   "error",
   "only target properties on the target schemas"
  ]
 ],
 "version": 1
}