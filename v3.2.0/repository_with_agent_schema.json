{
 "$schema": "http://www.archivesspace.org/archivesspace.json",
 "properties": {
  "agent_representation": {
   "type": "JSONModel(:agent_corporate_entity) object"
  },
  "create_time": {
   "readonly": true,
   "type": "date-time"
  },
  "created_by": {
   "readonly": true,
   "type": "string"
  },
  "jsonmodel_type": {
   "ifmissing": "error",
   "type": "string"
  },
  "last_modified_by": {
   "readonly": true,
   "type": "string"
  },
  "lock_version": {
   "required": false,
   "type": [
    "integer",
    "string"
   ]
  },
  "repository": {
   "ifmissing": "error",
   "type": "JSONModel(:repository) object"
  },
  "system_mtime": {
   "readonly": true,
   "type": "date-time"
  },
  "uri": {
   "required": false,
   "type": "string"
  },
  "user_mtime": {
   "readonly": true,
   "type": "date-time"
  }
 },
 "type": "object",
 "uri": "/repositories/with_agent",
 "version": 1
}