{
 "$schema": "http://www.archivesspace.org/archivesspace.json",
 "properties": {
  "create_time": {
   "readonly": true,
   "type": "date-time"
  },
  "created_by": {
   "readonly": true,
   "type": "string"
  },
  "date_expression": {
   "maxLength": 255,
   "type": "string"
  },
  "date_role": {
   "dynamic_enum": "date_role",
   "ifmissing": "error",
   "type": "string"
  },
  "date_standardized": {
   "maxLength": 255,
   "type": "string"
  },
  "date_standardized_type": {
   "dynamic_enum": "date_standardized_type",
   "required": "false",
   "type": "string"
  },
  "jsonmodel_type": {
   "ifmissing": "error",
   "type": "string"
  },
  "last_modified_by": {
   "readonly": true,
   "type": "string"
  },
  "lock_version": {
   "required": false,
   "type": [
    "integer",
    "string"
   ]
  },
  "repository": {
   "properties": {
    "_resolved": {
     "readonly": "true",
     "type": "object"
    },
    "ref": {
     "ifmissing": "error",
     "readonly": "true",
     "type": "JSONModel(:repository) uri"
    }
   },
   "readonly": "true",
   "subtype": "ref",
   "type": "object"
  },
  "system_mtime": {
   "readonly": true,
   "type": "date-time"
  },
  "user_mtime": {
   "readonly": true,
   "type": "date-time"
  }
 },
 "type": "object",
 "validations": [
  [
   "error",
   "check_structured_date_single"
  ]
 ],
 "version": 1
}